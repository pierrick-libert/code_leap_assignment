{
  "name": "ts-cat-example-api",
  "version": "1.0.0",
  "description": "Typescript Cat Example API created on a whim.",
  "main": "index.js",
  "scripts": {
    "start": "env TZ='Asia/Dubai' npx tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "compile": "npx tsc --alwaysStrict",
    "test": "npx mocha -r ts-node/register ./functional-test/*.test.ts",
    "lint": "npx eslint -c .eslintrc.js --ext .ts 'endpoints/*.ts' 'endpoints/**/*.ts' 'lib/*.ts' 'lib/**/*.ts' 'functional-test/*.ts'",
    "load_json": "npm run compile && node ./dist/load_json.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/10ten-me/ts-cat-example-api.git"
  },
  "keywords": [
    "typescript",
    "API",
    "CRUD"
  ],
  "author": "Pierrick Libert <pierrick.libert@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/10ten-me/ts-cat-example-api/issues"
  },
  "homepage": "https://github.com/10ten-me/ts-cat-example-api#readme",
  "dependencies": {
    "@sentry/node": "7.0.0",
    "@types/swagger-jsdoc": "6.0.1",
    "ajv": "8.11.0",
    "axios": "0.27.2",
    "cors": "2.8.5",
    "express": "4.18.1",
    "express-fileupload": "1.4.0",
    "pg": "8.7.3",
    "pg-native": "3.0.0",
    "probe-image-size": "7.2.3",
    "swagger-jsdoc": "6.2.1",
    "swagger-ui-express": "4.4.0",
    "ts-node": "10.8.0",
    "tsc-watch": "5.0.3",
    "typescript": "4.7.2",
    "uuid": "8.3.2",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-fileupload": "1.2.2",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.36",
    "@types/pg": "8.6.5",
    "@types/probe-image-size": "7.0.1",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "chai": "4.3.6",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "mocha": "10.0.0",
    "supertest": "6.2.3",
    "swagger-cli": "4.0.4",
    "ts-loader": "9.3.0",
    "tslint": "6.0.0"
  }
}
