{
  "name": "ts-seed-api",
  "version": "1.0.0",
  "description": "Typescript Seed API for all projects to handle CRUD operations",
  "main": "index.js",
  "scripts": {
    "start": "env TZ='Asia/Dubai' ./node_modules/.bin/tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "compile": "./node_modules/.bin/tsc --alwaysStrict",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register ./functional-test/*.test.ts",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js --ext .ts 'endpoints/*.ts' 'endpoints/**/*.ts' 'lib/*.ts' 'lib/**/*.ts' 'functional-test/*.ts'",
    "load_json": "npm run compile && node ./dist/load_json.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/10ten-me/ts-seed-api.git"
  },
  "keywords": [
    "typescript",
    "API",
    "CRUD"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/10ten-me/ts-seed-api/issues"
  },
  "homepage": "https://github.com/10ten-me/ts-seed-api#readme",
  "dependencies": {
    "@sentry/node": "6.16.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "ajv": "8.8.2",
    "axios": "0.24.0",
    "cors": "2.8.5",
    "express": "4.17.2",
    "express-fileupload": "1.2.1",
    "pg": "8.7.1",
    "pg-native": "3.0.0",
    "probe-image-size": "7.2.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "4.3.0",
    "ts-node": "10.4.0",
    "tsc-watch": "4.5.0",
    "typescript": "4.5.4",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-fileupload": "1.2.0",
    "@types/mocha": "9.0.0",
    "@types/node": "17.0.0",
    "@types/pg": "8.6.2",
    "@types/probe-image-size": "7.0.1",
    "@types/supertest": "2.0.11",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "chai": "4.3.4",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "mocha": "9.1.3",
    "supertest": "6.1.6",
    "swagger-cli": "4.0.4",
    "ts-loader": "9.2.6",
    "tslint": "6.0.0"
  }
}
