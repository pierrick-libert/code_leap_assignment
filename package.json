{
  "name": "ts-seed-api",
  "version": "1.0.0",
  "description": "Typescript Seed API for all projects to handle CRUD operations",
  "main": "index.js",
  "scripts": {
    "start": "env TZ='Asia/Dubai' ./node_modules/.bin/tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "compile": "./node_modules/.bin/tsc",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register ./functional-test/*.test.ts",
    "lint": "./node_modules/.bin/tslint --project tslint.json 'endpoints/*.ts' 'endpoints/**/*.ts' 'lib/*.ts' 'lib/**/*.ts' 'functional-test/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/10ten-me/ts-seed-api.git"
  },
  "keywords": [
    "typescript",
    "API",
    "CRUD"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/10ten-me/ts-seed-api/issues"
  },
  "homepage": "https://github.com/10ten-me/ts-seed-api#readme",
  "dependencies": {
    "@sentry/node": "6.14.0",
    "ajv": "8.6.3",
    "axios": "0.24.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-fileupload": "1.2.1",
    "jsonwebtoken": "8.5.1",
    "pg": "8.7.1",
    "pg-native": "3.0.0",
    "ts-node": "10.4.0",
    "tsc-watch": "4.5.0",
    "typescript": "4.4.4",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.22",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-fileupload": "1.1.7",
    "@types/faker": "5.5.9",
    "@types/jsonwebtoken": "8.5.5",
    "@types/mocha": "9.0.0",
    "@types/node": "15.6.2",
    "@types/pg": "8.6.1",
    "@types/supertest": "2.0.11",
    "chai": "4.3.4",
    "faker": "5.5.3",
    "mocha": "9.1.3",
    "supertest": "6.1.6",
    "ts-loader": "9.2.6",
    "tslint": "6.0.0"
  }
}
